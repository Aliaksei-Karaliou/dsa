<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="create-table-song-song" author="akaraliou">
        <createTable tableName="song-song">
            <column name="song1" type="int">
                <constraints nullable="false" foreignKeyName="fk_song_song_song" referencedColumnNames="song(id)"/>
            </column>
            <column name="song2" type="int">
                <constraints nullable="false" foreignKeyName="fk_song_song_song" referencedColumnNames="song(id)"/>
            </column>
            <column name="type" type="varchar(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <!--    paricipants-->

</databaseChangeLog>